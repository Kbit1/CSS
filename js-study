let pants = [20,22,24,26];
let shirts = [105,110,115];

$(".mt-2").on("click",function(){
    if($(this).val()=="ì…”ì¸ "){
        $(".form-select").eq(1).removeClass("mt-3")
        
    }
    else if ($(this).val()=="ë°”ì§€"){
        $(".form-select").eq(1).html(`
            <option>28</option>
            <option>30</option>
            <option>32</option>`)



           pants.forEach(function(a){
            $(".form-select").eq(1).append(`<option>${a}</option>`)
           })
    }
})
